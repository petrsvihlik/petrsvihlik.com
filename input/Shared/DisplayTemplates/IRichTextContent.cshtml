@model Kontent.Ai.Delivery.Abstractions.IRichTextContent

@foreach (var block in Model)
{
    @switch (block)
    {
        case Kontent.Ai.Delivery.Abstractions.IInlineImage image:
            @Html.DisplayFor(vm => image)
            break;

            
        case Kontent.Ai.Delivery.Abstractions.IHtmlContent html:
            @Html.Raw(html)
            break;

        default:
            @Html.DisplayFor(vm => block)
            break;
    }
}